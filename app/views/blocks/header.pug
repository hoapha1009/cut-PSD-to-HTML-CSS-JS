header.header
	.container 
		.header-top 
			a.header-logo(href="index.html") MoGo
			i.fa.fa-bars.header-toggle
			ul.header-menu
				+menuList("About")
				+menuList("Service")
				+menuList("Work")
				+menuList("Blog")
				+menuList("Contact")
				li.header-menu-item
					a.header-menu-icon(href="#")
						i.fa.fa-shopping-cart
				li.header-menu-item
					a.header-menu-icon(href="#")
						i.fa.fa-search
		.header-content 
			.header-content-caption Creative Template 
			.header-content-main Welcome to mogo 
			a.header-content-more(href="#") Learn more
		.header-bottom
			.header-bottom-item.is-active
				span.header-bottom-number 01
				span.header-bottom-text Intro
			+headerBottomItem("02", "work")
			+headerBottomItem("03", "about")
			+headerBottomItem("04", "contacts")
